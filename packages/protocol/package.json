    "./circuits/withdraw/target/vk.json": "./circuits/withdraw/target/vk.json",
    "./circuits/transfer/target/anon_transfer.json": "./circuits/transfer/target/anon_transfer.json",
    "./circuits/transfer/target/vk.json": "./circuits/transfer/target/vk.json"
  }
}
